{
    "manifest_version": 3,
    "name": "Twitch CSS Modifier",
    "version": "1.0.0",
    "description": "Disables background & overlay on Twitch",
    "icons": {
      "128": "twitch.png"
    },
    "content_scripts": [
      {
        "matches": ["*://*.twitch.tv/*"],
        "js": ["content.js"]
      }
    ]
  }